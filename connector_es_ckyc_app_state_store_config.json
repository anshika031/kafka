{
  "name": "es_ckyc_app_state_store",
  "config": {
    "connector.class": "io.confluent.connect.elasticsearch.ElasticsearchSinkConnector",
    "tasks.max": "3",
    "key.converter": "org.apache.kafka.connect.json.JsonConverter",
    "value.converter": "org.apache.kafka.connect.json.JsonConverter",
    "transforms": "extractKey, tsFormatter",
    "errors.retry.timeout": "3",
    "errors.tolerance": "all",
    "errors.log.enable": "true",
    "errors.log.include.messages": "true",
    "topics": "ckyc_app_state_store",
    "transforms.extractKey.type": "org.apache.kafka.connect.transforms.ExtractField$Key",
    "transforms.extractKey.field": "creationTime",
    "transforms.tsFormatter.type": "com.airtelbank.kafka.connect.smt.TimestampFormatter$Value",
    "transforms.tsFormatter.fields": "creationTime",
    "transforms.tsFormatter.target.format": "yyyy-MM-dd HH:mm:ss.SSSSSSZ",
    "connection.url": "https://apbsit21-142.airtelbank.com:9200",
    "connection.username": "dev",
    "connection.password": "Airtel@123",
    "flush.timeout.ms": "120000",
    "max.retries": "10",
    "read.timeout.ms": "20000",
    "key.ignore": "true",
    "schema.ignore": "true",
    "behavior.on.malformed.documents": "warn",
    "elastic.security.protocol": "PLAINTEXT",
    "type.name": "kafka-connect",
    "auto.create.indices.at.start": "false",
    "key.converter.schemas.enable": "false",
    "value.converter.schemas.enable": "false"
  }
}